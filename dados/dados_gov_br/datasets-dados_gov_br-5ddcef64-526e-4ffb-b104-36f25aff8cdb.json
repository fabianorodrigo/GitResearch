leiro.\r\n\r\n#### MISSÃO ####\r\n\r\n“Promover a saúde da população mediante a integração e a construção de parcerias com os órgãos federais, as unidades da Federação, os municípios, a iniciativa privada e a sociedade, contribuindo para a melhoria da qualidade de vida e para o exercício da cidadania\" ","id":"5b283f30-ced3-4ccc-b44a-406e8a92e1ad","image_url":"http://www.unasus.gov.br/sites/default/files/ministeriodasaude.jpg","is_organization":true,"name":"ministerio-da-saude-ms","revision_id":"c1a18b35-9c45-4f2a-ab8f-6dd8c0d650dc","state":"active","title":"Ministério da Saúde - MS","type":"organization"},"owner_org":"5b283f30-ced3-4ccc-b44a-406e8a92e1ad","private":false,"relationships_as_object":[],"relationships_as_subject":[],"resources":[{"cache_last_updated":null,"cache_url":null,"created":"2017-03-28T01:50:09.383256","datastore_active":true,"description":"","format":"CSV","hash":"","id":"8de68c56-b78e-483b-b430-0928ebd7b0eb","last_modified":null,"mimetype":"text/csv","mimetype_inner":null,"name":"Distribuição de UPAS Não Financiadas pelo PAC2 em Funcionamento","package_id":"5ddcef64-526e-4ffb-b104-36f25aff8cdb","position":0,"resource_type":null,"revision_id":"ad006340-42a6-40fa-a27d-f959e85c383e","size":null,"state":"active","url":"http://i3geo.saude.gov.br/i3geo/ogc.php?service=WFS&version=1.0.0&request=GetFeature&typeName=upa_pre_func&outputFormat=CSV","url_type":null,"webstore_last_updated":null,"webstore_url":null},{"cache_last_updated":null,"cache_url":null,"created":"2017-03-28T01:50:09.383268","datastore_active":false,"description":"","format":"GeoJSON","hash":"","id":"a0b1e687-9447-4911-b2e0-ae569c76d8d4","last_modified":null,"mimetype":"application/vnd.geo json","mimetype_inner":null,"name":"Distribuição de UPAS Não Financiadas pelo PAC2 em Funcionamento","package_id":"5ddcef64-526e-4ffb-b104-36f25aff8cdb","position":1,"resource_type":null,"revision_id":"ad006340-42a6-40fa-a27d-f959e85c383e","size":null,"state":"active","url":"http://i3geo.saude.gov.